<h2>プロフィール</h2>

<%= form_with model: @user do |f| %>
  <%= f.label :name ,"名前" %></th>
  <%= f.text_field :name %>

  <%= f.label :profile ,"プロフィール" %></th>
  <%= f.text_area :profile %>

  <ul>
    <li><%= f.submit "編集を完了する" %></li>
    <li><%= link_to "戻る", users_profile_path %></li>
  </ul>

<% end %>
